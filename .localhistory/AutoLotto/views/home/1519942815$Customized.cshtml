        @section scripts{
            <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Special+Elite" />
            <script>
                $(document).ready(function () {
                    $('map > area')
                        .mouseover(function () {
                            let imgId = $(this).attr("data-img-id");
                            $("#" + imgId).removeClass("hidden");
                            $("#" + imgId).css({ 'opacity': 0.3 });
                            $("#" + imgId + 0).css({ 'opacity': 0.3 });
                        }).mouseout(function () {
                            let imgId = $(this).attr("data-img-id");
                            $("#" + imgId).addClass("hidden");
                            $("#" + imgId + 0).css({ 'opacity': 1 });
                        }).click(function (e) {
                            e.preventDefault();
                            //$(this).unbind('mouseout');
                            let imgId = $(this).attr("data-img-id");
                            $("#" + imgId + '0').toggleClass("hidden");
                            console.log("egegs");
                        });

                    $('.difficultyLogo')
                        .mouseover(function () {
                            $(this).css({ 'opacity': 0.7 });

                        }).mouseout(function () {
                            $(this).css({ 'opacity': 0.5 });

                        }).click(function () {
                            $(this).off('mouseover');
                            $(this).off('mouseout');
                            $(this).css({ 'opacity': 1 });
                            let id = $(this).attr('id');
                            if (id == 'difficultyEasy') {
                                $('#difficultyMedium').css({ 'opacity': 0.5 });
                                $('#difficultyMedium').on('mouseover');
                                $('#difficultyMedium').on('mouseout');
                                $('#difficultyLarge').css({ 'opacity': 0.5 });
                                $('#difficultyLarge').bind('mouseover');
                                $('#difficultyLarge').bind('mouseout');
                            }
                            if (id == 'difficultyMedium') {
                                $('#difficultyEasy').css({ 'opacity': 0.5 });
                                $('#difficultyEasy').bind('mouseover');
                                $('#difficultyEasy').bind('mouseout');
                                $('#difficultyLarge').css({ 'opacity': 0.5 });
                                $('#difficultyLarge').bind('mouseover');
                                $('#difficultyLarge').bind('mouseout');
                            }
                            if (id == 'difficultyLarge') {
                                $('#difficultyMedium').css({ 'opacity': 0.5 });
                                $('#difficultyMedium').bind('mouseover');
                                $('#difficultyMedium').bind('mouseout');
                                $('#difficultyEasy').css({ 'opacity': 0.5 });
                                $('#difficultyEasy').bind('mouseover');
                                $('#difficultyEasy').bind('mouseout');
                            }
                        });
                });
            </script>
        }
